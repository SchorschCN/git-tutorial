git add
git commit -m
git commit -a -m
git log --pretty=oneline
git log --oneline
git reflog
git reset --hard [hash][HEAD^ ~NUM]
reset	--hard
	--mixed
	--soft
git diff [version] file //compare the specific file with repository [verison]
git diff [version1][version2] //compare version2 with version1, whats new in version2

git branch [name of new branch]
git checkout [branchname]
git branch -v
combine 2 branches:
	1.change to the destination branch
	2.git merge [source branch]
[modified by branch fix]

remote operations:
git remote -v
git remote add origin [url]	//username and passwd requested
git fetch [alias of remote repository] [name of remote branch]		//pull=fetch+merge
git merge [alias remote repo/remote branch]

